generator client {
  provider = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["summons", "geral"]
}


model Teste {
    id String @id @default(uuid())
    name String
    @@schema("geral")
}


model Teste1 {
    id String @id @default(uuid())
    name String
    @@schema("summons")
}
